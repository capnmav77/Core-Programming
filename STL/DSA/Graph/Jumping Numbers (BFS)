*/

#include<iostream>
#include<queue>
using namespace std;

void jump(int x) {
    queue<int> que;
    for(int i = 1; i<=9 && i<=x; i++) {
        que.push(i);
        while(!que.empty()) {
            int num = que.front();
            que.pop();
            if(num<=x) {
                cout << num << " ";
                int ld = num%10;
                if(ld == 0) {
                    que.push((num*10)+1);
                } else if (ld == 9) {
                    que.push((num*10)-1);
                } else {
                    que.push((num*10)+(ld-1));
                    que.push((num*10)+(ld+1));
                }
            }
        }
    }
}

int main() {
    int x = 20;
    cout << 0 << " ";
    jump(x);

}
